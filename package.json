{
  "name": "photo-archive-manager",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=16"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "type-check": "npm --workspaces run type-check",
    "lint": "eslint 'frontend/src/**/*.{ts,tsx}' 'backend/src/**/*.{ts,tsx}' --max-warnings=0",
    "format": "prettier --write 'frontend/src/**/*.{ts,tsx,json,md}' 'backend/src/**/*.{ts,tsx,json,md}'",
    "test": "npm --workspaces run test",
    "precommit": "lint-staged",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.1",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^8.0.0",
    "lint-staged": "^16.1.6",
    "prettier": "^3.6.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "express": "^5.1.0"
  }
}
